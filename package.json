{
  "name": "nest-cqrs-schematics",
  "version": "1.0.4",
  "description": "NestJS schematics for generating CQRS commands and queries",
  "main": "dist/index.js",
  "schematics": "./dist/collection.json",
  "scripts": {
    "build": "tsc -p tsconfig.json && cp src/collection.json dist/ && cp src/command/schema.json dist/command/ && cp src/query/schema.json dist/query/",
    "obfuscate": "javascript-obfuscator dist --output dist --compact true --control-flow-flattening true",
    "test": "npm run build && jasmine src/**/*_spec.js",
    "prepublishOnly": "npm run build && npm run obfuscate"
  },
  "keywords": [
    "nestjs",
    "cqrs",
    "schematics",
    "command",
    "query"
  ],
  "author": "Rameez Hasan <mrameezhasan@gmail.com>",
  "license": "MIT",
  "files": [
    "dist/**/*"
  ],
  "dependencies": {
    "@angular-devkit/core": "^20.1.0",
    "@angular-devkit/schematics": "^20.1.0",
    "typescript": "~5.8.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rameezhasanm/nestjs-cqrs-schematics/tree/main"
  },
  "devDependencies": {
    "@types/jasmine": "~5.1.0",
    "@types/node": "^20.17.19",
    "jasmine": "^5.0.0",
    "javascript-obfuscator": "^4.1.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
